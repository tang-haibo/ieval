{
  "name": "remote-import",
  "version": "1.0.0",
  "description": "js在微信小程序等环境实现远程载入其他实现库进行调用",
  "main": "./lib/",
  "scripts": {
    "dev": "./node_modules/.bin/rollup --config ./config/rollup.config.dev.ts --configPlugin typescript --watch",
    "build": "./node_modules/.bin/rollup --config ./config/rollup.config.prod.ts --configPlugin typescript",
    "release": "npm run jest && npm run build",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tang-haibo/remote-import.git"
  },
  "keywords": [
    "remote-import",
    "wx-import",
    "app-import"
  ],
  "author": "tang-haibo",
  "license": "(MIT OR Apache-2.0)",
  "bugs": {
    "url": "https://github.com/tang-haibo/remote-import/issues"
  },
  "homepage": "https://github.com/tang-haibo/remote-import#readme",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/jest": "^26.0.24",
    "@types/jsdom": "^16.2.13",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.4.13",
    "coveralls": "^3.1.1",
    "jest": "^27.0.6",
    "jsdom": "^16.7.0",
    "rollup": "^2.54.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "acorn": "^8.3.0"
  }
